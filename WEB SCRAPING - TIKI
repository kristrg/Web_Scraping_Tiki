G ###\n","######################\n","\n","# SCRAPING ONE CATEGORY\n","def scrape_one_cat(main_cat_url,num_max_page):\n","    # num_max_page = 5\n","    # main_cat_url = MAIN_CATEGORIES[0]['URL']\n","    seperator_index = main_cat_url.find(\"?\")\n","    # Another way to split:\n","    # main_url,opt_url = main_cat_url.split(\"?\")\n","    final_df = pd.DataFrame()\n","    start_driver(force_restart=True)\n","    for i in range(1,num_max_page):\n","        branch_cat_url = main_cat_url[:(seperator_index+1)] + f'page={i}&' + main_cat_url[(seperator_index+1):]\n","\n","        #CODE TO GET DATA\n","        prod_data = get_product_info_from_page(branch_cat_url)\n","\n","        #CONVERT DATA TO A DATAFRAME\n","        df = pd.DataFrame(data=prod_data, columns=prod_data[0].keys())\n","        final_df = pd.concat([final_df,df],axis=0,ignore_index=True)\n","        # df.to_csv('tiki_products.csv')\n","    \n","    close_driver()\n","\n","    return final_df\n"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"2gR6e62ghuSE","executionInfo":{"status":"ok","timestamp":1633274558230,"user_tz":-420,"elapsed":2154616,"user":{"displayName":"Trương Hoàng Đông","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gi2u3kwwy34544bIzkBDG3wU-ziQ4Q1mDnYV03J3A=s64","userId":"13637122635268472990"}},"outputId":"98663459-04e2-4dae-eb80-343bce5d2f9b"},"source":["# SCRAPING MULTIPLE CATEGORIES\n","\n","total_products_df = pd.DataFrame()\n","\n","for i in range(len(MAIN_CATEGORIES)):\n","\n","    print(\"Crawling thru Category: \"+MAIN_CATEGORIES[i]['Name'])\n","    df = scrape_one_cat(MAIN_CATEGORIES[i][\"URL\"],6)\n","    total_products_df = pd.concat([total_products_df,df],axis = 0, ignore_index=True)"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Crawling thru Category: Điện Thoại - Máy Tính Bảng\n","Found 72 products\n","Found 72 products\n","Found 72 products\n","Found 72 pro