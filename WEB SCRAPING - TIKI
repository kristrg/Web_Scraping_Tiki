# Wrapper to close driver if its created\n","def close_driver():\n","    global DRIVER\n","    if DRIVER is not None:\n","        DRIVER.close()\n","    DRIVER = None"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"9ur0CQW0XRcI"},"source":["#Function to get info from one product"]},{"cell_type":"code","metadata":{"id":"a3lx4DNUXQhN"},"source":["#################\n","### FUNCTIONS ###\n","#################\n","\n","\n","# Function to extract product info from the product\n","# product_item trong TH nay la 1 single product\n","def get_product_info_single(product_item):\n","    d = {'name':'',\n","         'price':'',\n","         'product_url':'',\n","         'image':'',\n","         'freeship': '',\n","         'average_review':'',\n","         'number_of_sales':'',\n","         'sale':'',\n","         'tikinow':'',\n","         \"tikinow_member_deal\":\"\",\n","         \"re_hon_hoan_tien\":'',\n","         \"tra_gop\":'',\n","         \"free_gift\":''}\n","\n","    \n","    # name get name through find_element_by_class_name\n","    try:\n","        span_tag = product_item.find_element_by_class_name(\"name\").find_element_by_tag_name(\"span\")\n","        d['name'] = span_tag.get_attribute(\"innerHTML\")\n","    except NoSuchElementException:\n","        pass\n","\n","    # get price find_element_by_class_name\n","    try:\n","        d['price'] = product_item.find_element_by_class_name(\"price-discount__price\").text\n","    except NoSuchElementException:\n","        d['price'] = -1\n","    if d['price'] == '':\n","        price_tag = product_item.find_element_by_class_name(\"price-discount__price\")\n","        d['price'] = price_tag.get_attribute(\"innerHTML\")\n","    \n","    # get link from .get_attribute()\n","    try:\n","        product_link     = product_item.get_attribute(\"href\")\n","        d['product_url'] = product_link #YOUR CODE HERE CLEAN LINK WITH STRING MANUPULATION\n","    except NoSuchElementException:\n","        pass\n","    \n","    # get thumbnail by class_name and Tag name and get_attribute()\n","    try:\n","        thumbnail =  product_item.find_element_by_class_name(\"webpimg-container\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d['image'] = thumbnail #YOUR CODE HERE\n","    except NoSuchElementException:\n","        pass\n","\n","    # get thumbnail freeship\n","    try:\n","        freeship_thumbnail = product_item.find_element_by_class_name(\"thumbnail\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d[\"freeship\"] = freeship_thumbnail\n","        real_free = \"https://salt.tikicdn.com/ts/upload/dc/0d/49/3251737db2de83b74eba8a9ad6d03338.png\"\n","        if d[\"freeship\"] in real_free:\n","            d[\"freeship\"] = \"Yes\"\n","        else:\n","            d[\"freeship\"] = \"No\"\n","    except NoSuchElementException:\n","        pass\n","\n","    # get average review\n","    try:\n","        average_review = product_item.find_element_by_class_name(\"average\").get_attribute(\"style\")\n","        d[\"average_review\"] = average_review.strip(\"width:;\")\n","    except NoSuchElementException:\n","        pass\n","    \n","    # get # of sold\n","    try:\n","        no_of_sales = product_item.find_element_by_class_name(\"styles__StyledQtySold-sc-732h27-2\").text\n","        d[\"number_of_sales\"] = no_of_sales.strip(\"Đã bán +\")\n","    except NoSuchElementException:\n","        pass\n","\n","    # Sale rate\n","    try:\n","        sale_rate = product_item.find_element_by_class_name(\"price-discount__discount\").text\n","        d['sale'] = sale_rate\n","    except NoSuchElementException:\n","        pass\n","\n","    # Tikinow\n","    try:\n","        tikinow_thumbnail = product_item.find_element_by_class_name(\"badge-service\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d['tikinow'] = tikinow_thumbnail\n","        tikinow_src = \"https://salt.tikicdn.com/ts/upload/9f/32/dd/8a8d39d4453399569dfb3e80fe01de75.png\"\n","        if d[\"tikinow\"] in tikinow_src:\n","            d[\"tikinow\"] = \"Yes\"\n","        else:\n","            d[\"tikinow\"] = \"No\"\n","    except NoSuchElementException:\n","        pass\n","\n","    # Tikinow member price deal badge\n","    try:\n","        tikinowmemdeal_thumbnail = product_item.find_element_by_class_name(\"badge-benefits\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d['tikinow_member_deal'] = tikinowmemdeal_thumbnail\n","        tikinowmemdeal_src = \"https://salt.tikicdn.com/ts/upload/ba/9e/0d/9edd3b5458502375eb0cedf17acb732f.png\"\n","        if d[\"tikinow_member_deal\"] in tikinowmemdeal_src:\n","            d[\"tikinow_member_deal\"] = \"Yes\"\n","        else:\n","            d[\"tikinow_member_deal\"] = \"No\"\n","    except NoSuchElementException:\n","        pass\n","\n","\n","    # Re hon hoan tien badge\n","    try:\n","        rehonhoantien_thumbnail = product_item.find_element_by_class_name(\"badge-under-price\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d['re_hon_hoan_tien'] = rehonhoantien_thumbnail\n","        rehonhoantien_src = \"https://salt.tikicdn.com/ts/upload/51/ac/cc/528e80fe3f464f910174e2fdf8887b6f.png\"\n","        if d[\"re_hon_hoan_tien\"] in rehonhoantien_src:\n","            d[\"re_hon_hoan_tien\"] = \"Yes\"\n","        else:\n","            d[\"re_hon_hoan_tien\"] = \"No\"\n","    except NoSuchElementException:\n","        pass\n","\n","    # Installment badge\n","    try:\n","        tragop_thumbnail = product_item.find_element_by_class_name(\"badge-benefits\").find_element_by_tag_name(\"img\").get_attribute(\"src\")\n","        d['tra_gop'] = tragop_thumbnail\n","        tragop_src = \"https://salt.tikicdn.com/ts/upload/ba/4e/6e/26e9f2487e9f49b7dcf4043960e687dd.png\"\n","        if d[\"tra_gop\"] in tragop_src:\n","            d[\"tra_gop\"] = \"Yes\"\n","        else:\n","            d[\"tra_gop\"] = \"No\"\n